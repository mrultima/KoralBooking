{
  "name": "my-booking",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:watch": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production --stats-json --watch",
    "analyze": "webpack-bundle-analyzer dist/my-booking/stats.json",
    "serve": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng serve --port=443 --host 0.0.0.0 --disableHostCheck --ssl --live-reload false",
    "prod": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production --stats-json",
    "dev:ssr": "ng run my-booking:serve-ssr",
    "serve:ssr": "node dist/my-booking/server/main.js",
    "build:ssr": "ng build --configuration production && ng run my-booking:server:production",
    "prerender": "ng run my-booking:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.7",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "~12.2.7",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/platform-server": "~12.2.7",
    "@angular/router": "~12.2.7",
    "@nguniversal/express-engine": "^12.1.0",
    "angular8-yandex-maps": "^12.1.7",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.7",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
